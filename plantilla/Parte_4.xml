                                                <script type='text/javascript'>
                                                    /*<![CDATA[*/
                                                    var relatedPosts = [];
                                                    var genericThumbnail = "https://blogger.googleusercontent.com/img/b/R29vZ2xl/AVvXsEinnB339oLvhMOq3gq0c7uSJkTUuqcD02V8xm0KWuBtAUjJ20PQqYCGDz2Yq1XG8PVomtvBPgBFAT_5vwzoyGJ_bOsg1X3FsG3SvpBA_V7MrzbBYGBPjXa5xHbmJ-VfuaSE1IptAsPuvl3tshnVD7h1XpU38e16BZqVF80QmFmOa_6FtREhUnCr_pEQYLBv/s1600/no-image.webp";
                                                    var currentUrl = window.location.href.replace(/#.*/, "");

                                                    function related_results_labels(json) {
                                                        if (!json.feed || !json.feed.entry) return;
                                                        for (var i = 0; i < json.feed.entry.length; i++) {
                                                            var entry = json.feed.entry[i];
                                                            var entryUrl = "";
                                                            for (var j = 0; j < entry.link.length; j++) {
                                                                if (entry.link[j].rel === "alternate") {
                                                                    entryUrl = entry.link[j].href;
                                                                    break;
                                                                }
                                                            }
                                                            if (entryUrl === currentUrl) continue;
                                                            var thumb = "";
                                                            var hasImage = false;
                                                            if (entry.media$thumbnail && entry.media$thumbnail.url) {
                                                                thumb = entry.media$thumbnail.url.replace(/\/s72-c\//, "/s160-c/");
                                                                hasImage = true;
                                                            }
                                                            if (!relatedPosts.some(function(r) { return r.url === entryUrl; }))
                                                                relatedPosts.push({
                                                                    title: entry.title.$t,
                                                                    url: entryUrl,
                                                                    thumb: thumb,
                                                                    hasImage: hasImage
                                                                });
                                                        }
                                                    }

                                                    function shuffleArray(array) {
                                                        for (let i = array.length - 1; i > 0; i--) {
                                                            const j = Math.floor(Math.random() * (i + 1));
                                                            [array[i], array[j]] = [array[j], array[i]];
                                                        }
                                                    }

                                                    function showRelatedBlock() {
                                                        if (!relatedPosts.length) return;
                                                        shuffleArray(relatedPosts);
                                                        var container = document.createElement('div');
                                                        container.className = 'suggested-posts';
                                                        var html = "<div class='suggested-title'>You'll Also Like</div><ul style='list-style:none;padding:0;margin:0'>";
                                                        for (var i = 0; i < relatedPosts.length && i < 2; i++) {
                                                            var imageHtml = "";
                                                            if (relatedPosts[i].hasImage) {
                                                                imageHtml = "<img src='" + relatedPosts[i].thumb + "' alt='" + relatedPosts[i].title + "' loading='lazy'/>";
                                                            } else {
                                                                imageHtml = "<div class='no-image-placeholder'></div>";
                                                            }
                                                            html += "<li style='display:flex;align-items:center;margin-bottom:8px;'><a href='" + relatedPosts[i].url + "'  style='display:flex;align-items:center;text-decoration:none;color:inherit;'>" + imageHtml + "<span>" + relatedPosts[i].title + "</span></a></li>";
                                                        }
                                                        html += "</ul>";
                                                        container.innerHTML = html;

                                                        var postBody = document.getElementById('mainPostBody');
                                                        var ps = postBody ? postBody.getElementsByTagName('p') : [];
                                                        if (ps.length >= 3) {
                                                            ps[2].parentNode.insertBefore(container, ps[2].nextSibling);
                                                        } else if (postBody) {
                                                            postBody.appendChild(container);
                                                        }
                                                    }

                                                    function waitForRelated() {
                                                        if (window.relatedLabelsLoaded) {
                                                            showRelatedBlock();
                                                        } else {
                                                            setTimeout(waitForRelated, 100);
                                                        }
                                                    }

                                                    window.relatedLabelsLoaded = false;

                                                    document.addEventListener("DOMContentLoaded", function() {
                                                        waitForRelated();
                                                    });
                                                    /*]]>*/
                                                </script>
