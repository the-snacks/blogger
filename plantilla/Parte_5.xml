                          /*<![CDATA[*/
                          (function() {
                            // Etiquetas bloqueadas (en minúsculas, sin acentos)
                            var etiquetasBloqueadas = ["courses", "products", "estilo pop"];

                            // Función para normalizar: minúsculas + sin acentos
                            function normalizar(str) {
                              return str
                                .toLowerCase()
                                .normalize("NFD")
                                .replace(/[\u0300-\u036f]/g, ""); // quita tildes
                            }

                            // Obtener etiquetas visibles del post
                            var etiquetasPost = Array.from(
                              document.querySelectorAll('a[rel="tag"], a[href*="/search/label/"]')
                            ).map(a => normalizar(a.textContent.trim()));

                            // Si alguna coincide, salir
                            if (etiquetasPost.some(tag => etiquetasBloqueadas.includes(tag))) return;

                            // Configuración
                            var minParrafosGlobal = 8;
                            var distanciaMinima = 4;
                            var bloques = [
                              { id: 'HTML01', posicion: 0.5, prioridad: 1 },
                              { id: 'HTML02', posicion: 0.8, prioridad: 2 }
                            ];
                            var selectoresProhibidos = ['ul', 'ol', 'blockquote', 'details', 'figure'];

                            var postBody = document.getElementById('postBody');
                            if (!postBody) return;

                            var parrafos = Array.from(postBody.querySelectorAll('p')).filter(function(p) {
                              return !selectoresProhibidos.some(sel => p.closest(sel)) &&
                                     p.textContent.trim().length > 0;
                            });

                            if (parrafos.length < minParrafosGlobal) return;

                            function insertarBloque(bloque, indice) {
                              var adEl = document.getElementById(bloque.id);
                              if (!adEl || adEl.innerHTML.trim() === '') return;
                              parrafos[indice].insertAdjacentElement('afterend', adEl);
                            }

                            var ultimaPosicion = -Infinity;
                            bloques.sort((a,b) => a.prioridad - b.prioridad).forEach(function(bloque) {
                              var indiceIdeal = Math.floor(parrafos.length * bloque.posicion);
                              if (indiceIdeal >= parrafos.length) indiceIdeal = parrafos.length - 1;

                              var indiceFinal = indiceIdeal;
                              if (indiceFinal - ultimaPosicion < distanciaMinima) {
                                var adelante = indiceIdeal + distanciaMinima;
                                var atras = indiceIdeal - distanciaMinima;
                                if (adelante < parrafos.length) indiceFinal = adelante;
                                else if (atras > ultimaPosicion) indiceFinal = atras;
                                else return;
                              }

                              insertarBloque(bloque, indiceFinal);
                              ultimaPosicion = indiceFinal;
                            });
                          })();
                          /*]]>*/
                          </script>

                        </div>
                      </b:includable>
